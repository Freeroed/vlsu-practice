
entity Todo {
	name String required,
    description String,
    createdAt Instant,
    deleted Boolean,
    duration Integer
    
    
}

paginate Todo with pagination

// Use Data Transfer Objects (DTO)
// dto * with mapstruct

// Set service options to all except few
service all with serviceImpl
dto all with mapstruct

